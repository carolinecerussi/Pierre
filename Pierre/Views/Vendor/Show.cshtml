@{
  Layout = "_Layout";
}

@using Pierre.Models;

<h1>Here are all of the Orders with this Vendor</h1>


<ol>
  @foreach (Order order in @Model["order"])
  {
    <li><a href='/vendor/@Model["vendor"]/order/@order.Oid'>@Order.OrderName</a></li>
  }
</ol>


<p><a href='/vendor/@Model["vendor"].Id/order/new'>Add a new Order</a></p>

<p><a href='/vendor'>Return to Vendors</a></p>

